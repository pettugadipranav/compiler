roots_of_unity cdouble[] :(double n) {
    int y;
    cdouble roots[y] ;
    double PI=3.14;
    iter (int i = 0; i < n; i++) {
        roots[i] = (real(2 * PI * i / n), img(2 * PI * i / n));
    }
    return roots;
}
main int :() {
    double n = 4;
    cdouble roots[4] ;
    roots= roots_of_unity(n);
    return 0;
}
